return {
  "nvim-tree/nvim-tree.lua",
  version = "*",
  lazy = false,
  dependencies = {
    "nvim-tree/nvim-web-devicons",
  },
  config = function()
    require("nvim-tree").setup {}
    local tree = require("nvim-tree.api").tree
    vim.keymap.set("n", "<leader>n", tree.toggle, {})
    vim.keymap.set("n", "<leader>e", tree.focus, {})
  end,
}
